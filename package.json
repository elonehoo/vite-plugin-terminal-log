{
  "name": "vite-plugin-terminal-log",
  "type": "module",
  "version": "0.0.1",
  "description": "When it is convenient for development, the front-end routine log can be printed to the terminal output panel",
  "author": "Elone Hoo <elonehoo@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/elonehoo/vite-plugin-terminal-log",
  "repository": {
    "type": "git",
    "url": "https://github.com/elonehoo/vite-plugin-terminal-log"
  },
  "bugs": "https://github.com/elonehoo/vite-plugin-terminal-log/issues",
  "keywords": [
    "vite",
    "plugin",
    "terminal",
    "Log"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "engines": {
    "node": ">=22.13.1"
  },
  "scripts": {
    "play": "pnpm -C playground run dev",
    "dev": "unbuild --stub",
    "build": "unbuild",
    "stale": "stale-del -u",
    "test": "vitest",
    "ui": "vitest --ui",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp package.json --commit --push --tag && pnpm -r publish --access public"
  },
  "dependencies": {
    "kolorist": "^1.8.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.2.0",
    "@types/node": "^22.13.1",
    "@vitest/ui": "^2.1.9",
    "bumpp": "^9.11.1",
    "eslint": "^9.20.0",
    "nodemon": "^3.1.9",
    "statl-del": "^0.0.4",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "vite": "^6.1.0",
    "vitest": "^2.1.9"
  }
}
